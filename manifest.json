{
  "name": "Bookmarks Cleaner",
  "version": "1.0.0",
  "description": "Clean 404 bookmarks",
  "permissions": [
    "bookmarks",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
      "default_title": "Bookmarks Cleaner",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com https://maxcdn.bootstrapcdn.com; object-src 'self'"
}
